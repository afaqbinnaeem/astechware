<%# Menu JS %>
<script>
   const bodyTag = document.querySelector("body");
   const buttonTag = document.querySelector(".nav-button");
   let count = 0;
   let socialCount = 0;
   
   buttonTag.addEventListener("click", function(event) { count = 0 })
   bodyTag.addEventListener("click", function(event) {  
     if (bodyTag.classList.contains('open-dropdown')  && count > 0) {
       bodyTag.classList.remove("open-dropdown");
       buttonTag.classList.remove("cmenu");
       count = 0
     }else {
       count++;
     }
   
     if (bodyTag.classList.contains('social-active') && socialCount > 0) {
       bodyTag.classList.remove("social-active");
       socialCount = 0
     }else {
       socialCount++;
     }
   });
</script>